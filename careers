<script defer>
    $(document).ready(function() {

        var selectMenu = $('select');

        $('.our-offices-slider, .workers-slider-init').slick({
            lazyLoad: 'progressive',
            dots: true
        });

        selectMenu.select2({
            minimumResultsForSearch: -1,
            dropdownCssClass: 'withIcon',
            dropdownParent: $('.our-offices-dropdown-modal')
        });

        $('.citySelect').val('tallinn').change();
        var cityDesc = $('.tallinn');
        $('.our-offices-desc').height(cityDesc.height());

        selectMenu.on('change', function() {
            cityDesc.find('.our-offices-images').addClass('invisible');
            cityDesc.removeClass('active btn-secondary').addClass('btn-outline-secondary');
            var currentOption = $(".citySelect :selected");
            cityDesc = $('.' + currentOption.val());
            loadImg(cityDesc);
            $('.btn-buttons-list').removeClass('active btn-secondary').addClass('btn-outline-secondary');
            cityDesc.addClass('active');
            $('[data-city=' + currentOption.val() + ']').addClass('active btn-secondary').removeClass('btn-outline-secondary');
            cityDesc.find('.our-offices-images').removeClass('invisible');
            $('.our-offices-desc').height(cityDesc.height());
        });

        $('.btn-buttons-list').on('click', function(e) {
            if (e.target.classList.contains('active')) return;
            var id = $(this).attr("data-city");
            $(".citySelect").val(id).change();
        });

        $(window).scroll(function() {
            removeIsVisibleClass();
        });

        $(window).resize(function() {
            $('.our-offices-desc').height(cityDesc.height());
        });

        function loadImg(currentOfficeDescription) {
            currentOfficeDescription.find('.our-offices-images > img').each(function (index) {
                if ($(this).attr('src')) return;
                var imgURL = $(this).attr('data-lazy-load');
                $(this).attr('src', imgURL);
            });
        }

        function removeIsVisibleClass() {
            if (isVisible) return;
            var hT = $('.scroll-to-city').offset().top,
                hH = $('.scroll-to-city').outerHeight(),
                wH = $(window).height(),
                wS = $(this).scrollTop();
            if (wS > (hT+hH-wH)) {
                $('.tallinn .our-offices-images').removeClass('invisible');
                isVisible = true;
            }
        }

        $('#videoModal').on('hidden.bs.modal', () => {
            $('#popup-youtube-player')[0].contentWindow.postMessage('{"event":"command","func":"' + 'pauseVideo' + '","args":""}', '*');
        })

        var isVisible = false;
        removeIsVisibleClass();
    })
</script>

