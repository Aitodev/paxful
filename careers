            $(document).ready(function() {
                var pageBody = $('body');
                $(".nav-btn").on('click', function() {
                    $('.careers-header').toggleClass('open-menu');
                    pageBody.toggleClass('open-menu');
                    pageBody.hasClass('open-menu') ? pageBody.css('overflow-y', 'hidden') : pageBody.css('overflow-y', 'scroll');
