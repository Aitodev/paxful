                $(".whats-new-content-cards-wrapper").addClass("d-none")
            }
            if (results && results.cards && results.cards.length > 0) {
                $(".whats-new-content-cards-wrapper").removeClass("d-none")
            }
        })
        appboy.requestContentCardsRefresh();
    </script>
