                    }
                },
                zIndex: 1020,
            }
        };

        zE("webWidget", "setLocale", "en-us");

        zE("webWidget:on", "close", function() {
            toggleMobileWidget();
            clearTimeout(clearFormTimeout);
            zE('webWidget', 'clear');
        });

