    document.addEventListener("DOMContentLoaded", function() {
        var clearFormTimeout;
        var width = window.innerWidth;
        var widthRange = [width, width - 1, width + 1];

        function toggleMobileWidget() {
            if (location.href !== "https://paxful.com/zh/" && window.Paxful && window.Paxful.app && window.Paxful.app.isMobileApp === false) {
                if (window.innerWidth < 667) {
                    zE('webWidget', 'hide');
                } else {
                    zE('webWidget', 'show');
                }
            }
        }

        toggleMobileWidget();

        window.addEventListener('resize', function() {
            if (widthRange.includes(window.innerWidth)) return;
            toggleMobileWidget();
        });

        window.zESettings = {
            webWidget: {
                launcher: {
                    label: {
                        '*': "联系我们",
                    },
                },
