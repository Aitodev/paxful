        var clearFormTimeout;
        var width = window.innerWidth;
        var widthRange = [width, width - 1, width + 1];

        function toggleMobileWidget() {
            if (location.href !== "https://paxful.com/fr/" && window.Paxful && window.Paxful.app && window.Paxful.app.isMobileApp === false) {
                if (window.innerWidth < 667) {
