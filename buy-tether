            <script type="text/javascript" src="../2/js/plugins/jquery-1.12.4.min.js"></script>
            <script src="../js/moment-with-locales.min.js"></script>
            <script src="../js/moment-timezone.min.js"></script>
            <script> moment.locale('ru'); </script>
