<script type="text/javascript">
    document.addEventListener("DOMContentLoaded", function() {
        var clearFormTimeout;
        var width = window.innerWidth;
        var widthRange = [width, width - 1, width + 1];

        function toggleMobileWidget() {
            if (location.href !== "https://paxful.com/ru/" && window.Paxful && window.Paxful.app && window.Paxful.app.isMobileApp === false) {
                if (window.innerWidth < 667) {
                    zE('webWidget', 'hide');
                } else {
                    zE('webWidget', 'show');
                }
            }
        }

        toggleMobileWidget();

        window.addEventListener('resize', function() {
            if (widthRange.includes(window.innerWidth)) return;
            toggleMobileWidget();
        });

        window.zESettings = {
            webWidget: {
                launcher: {
                    label: {
                        '*': "Контакты",
                    },
                },
                chat: {
                    suppress: true
                },
                contactForm: {
                    title: {
                        "*": "Связаться с Paxful",
                    },
                },
                helpCenter: {
                    suppress: true
                },
                color: {
                    launcher: '#7f59ad',
                    launcherText: '#FFFFFF',
                },
                offset: {
                    horizontal: '0',
                    vertical: 0,
                    mobile: {
                        horizontal: '0',
                        vertical: 0,
                    }
                },
                zIndex: 1020,
            }
        };

        zE("webWidget", "setLocale", "ru");

        zE("webWidget:on", "close", function() {
            toggleMobileWidget();
            clearTimeout(clearFormTimeout);
            zE('webWidget', 'clear');
        });

        zE('webWidget:on', 'open', function() {
            
            // Clearing widget if page opened for 2 hours
            clearFormTimeout = setTimeout(function(){
                zE('webWidget', 'clear');
            }, 1000 * 60 * 60 * 2);
        });
            });
</script>


    
