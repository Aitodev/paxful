    function generateWeekArray() {
        var resultArray = [];
        for (var i = FIRST_DAY_OF_WEEK; i <= LAST_DAY_OF_WEEK; i++) {
            resultArray.push({ day: i, active: 1, start_time: "00:00", end_time: "00:00" });
        }
        return resultArray;
    }
    window.Paxful.createOfferOpenHours = generateWeekArray();
    window.Paxful.generalOpenHoursSettings = {
        isScheduleAppliedToAllOffers: false,
        timezone: null
    };
</script>
    <script type="text/javascript">
        var duty_hours = [];
        window.Paxful.createOfferOpenHours = duty_hours.length === 0 ? generateWeekArray() : duty_hours;
    </script>
    
